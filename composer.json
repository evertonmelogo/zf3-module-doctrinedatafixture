{
    "name": "ise/zf2-module-doctrinedatafixture",
    "description": "Zend Framework 2 Module that provides Doctrine Data-Fixture functionality",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "doctrine",
        "data-fixture",
        "module",
        "zf2"
    ],
    "homepage": "https://github.com/internalsystemerror/zf2-module-doctrinedatafixture",
    "authors": [
        {
            "name": "Martin Shwalbe",
            "email": "martin.shwalbe@gmail.com"
        },
        {
            "name": "ise",
            "email": "info@ise.me.uk"
        }
    ],
    "require": {
        "zendframework/zend-modulemanager": "^2.5",
        "zendframework/zend-eventmanager": "^2.5 || ^3.0",
        "zendframework/zend-servicemanager": "^2.5 || ^3.0",
        "doctrine/data-fixtures": "^1.0",
        "doctrine/doctrine-orm-module": "^1.0"
    },
    "require-dev": {
        "ise/php-metapackage-development": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DoctrineDataFixtureModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DoctrineDataFixtureModuleTest\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.1.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit -c ./phpunit.xml.dist",
        "cs-fix": "phpcbf",
        "cs-check": [
            "phpcs",
            "phpmd ./config,./src,./test, html --reportfile ./build/logs/phpmd.html --suffixes php,phtml ./phpmd.xml.dist",
            "phpcpd . --exclude=build --exclude=vendor"
        ]
    }
}
